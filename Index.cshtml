@{
    ViewData["Title"] = "Home Page";
}
@model List<Dish>

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <div>
   <a asp-controller = "Home" asp-action="Index">Home</a>
     <a asp-controller="Home"  asp-action="NewDish" method="GET">Add a Dish</a>
</div>
</div>
@foreach (Dish dish in Model){
    <div>
        
        <a asp-controller="Home" asp-action="ViewDish" asp-route-id = "@dish.DishId">@dish.Name </a>
        <p>by @dish.Chef </p>


     
    </div>
   
}
